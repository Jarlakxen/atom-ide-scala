{
  "name" : "ide-scala",
  "main" : "./lib/main.js",
  "version" : "0.4.0",
  "description" : "Scala language support for Atom-IDE",
  "author" : {
    "name" : "Alexey Alekhin",
    "email" : "laughedelic@gmail.com",
    "url" : "https://github.com/laughedelic"
  },
  "keywords" : [ "scala", "scalameta", "ide", "atom-ide", "lsp", "language-server", "language-server-protocol" ],
  "repository" : "https://github.com/laughedelic/atom-ide-scala",
  "bugs" : "https://github.com/laughedelic/atom-ide-scala/issues",
  "license" : "MIT",
  "engines" : {
    "atom" : ">=1.21.0 <2.0.0"
  },
  "enhancedScopes" : [ "source.scala" ],
  "dependencies" : {
    "atom-languageclient" : "0.7.0",
    "find-java-home" : "0.2.0"
  },
  "consumedServices" : {
    "linter-indie" : {
      "versions" : {
        "2.0.0" : "consumeLinterV2"
      }
    },
    "datatip" : {
      "versions" : {
        "0.1.0" : "consumeDatatip"
      }
    },
    "atom-ide-busy-signal" : {
      "versions" : {
        "0.1.0" : "consumeBusySignal"
      }
    },
    "signature-help" : {
      "versions" : {
        "0.1.0" : "consumeSignatureHelp"
      }
    }
  },
  "providedServices" : {
    "definitions" : {
      "versions" : {
        "0.1.0" : "provideDefinitions"
      }
    },
    "code-highlight" : {
      "versions" : {
        "0.1.0" : "provideCodeHighlight"
      }
    },
    "hyperclick" : {
      "versions" : {
        "0.1.0" : "provideHyperclick"
      }
    },
    "find-references" : {
      "versions" : {
        "0.1.0" : "provideFindReferences"
      }
    },
    "autocomplete.provider" : {
      "versions" : {
        "2.0.0" : "provideAutocomplete"
      }
    },
    "code-format.range" : {
      "versions" : {
        "0.1.0" : "provideCodeFormat"
      }
    },
    "outline-view" : {
      "versions" : {
        "0.1.0" : "provideOutlines"
      }
    }
  },
  "configSchema" : {
    "serverType" : {
      "order" : 1,
      "type" : "string",
      "title" : "Language Server Type",
      "description" : "Don't change this option unless you know what you're doing",
      "default" : "scalameta",
      "enum" : [ {
        "value" : "scalameta",
        "description" : "Scalameta"
      }, {
        "value" : "ensime",
        "description" : "ENSIME (experimental)"
      } ]
    },
    "serverVersion" : {
      "order" : 2,
      "type" : "string",
      "title" : "Language Server Version",
      "default" : "1ebc5392"
    },
    "jvm" : {
      "order" : 3,
      "type" : "object",
      "title" : "Java-related settings",
      "properties" : {
        "javaHome" : {
          "type" : "string",
          "title" : "Java Home",
          "description" : "Plugin will try to guess your Java Home path, but if you have a very specific setup you can use this option to set it explicitly",
          "default" : ""
        },
        "javaOpts" : {
          "type" : "array",
          "title" : "Extra JVM options",
          "default" : [ ],
          "items" : {
            "type" : "string"
          }
        }
      }
    }
  }
}